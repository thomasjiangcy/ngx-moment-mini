{
  "name": "ngx-moment-mini",
  "version": "3.1.1",
  "description": "Moment.JS pipes for Angular (timeago and more) with moment-mini",
  "scripts": {
    "build": "ng build",
    "test": "ng lint && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk --inspect ./node_modules/jest/bin/jest.js --runInBand",
    "prepublish": "ng build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasjiangcy/ngx-moment-mini.git"
  },
  "keywords": [
    "angular",
    "timeago",
    "momentjs"
  ],
  "author": "Uri Shaked",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/thomasjiangcy/ngx-moment-mini/issues"
  },
  "homepage": "https://github.com/thomasjiangcy/ngx-moment-mini#readme",
  "peerDependencies": {
    "@angular/common": ">=2.0.0 <=7.0.0",
    "@angular/core": ">=2.0.0 <=7.0.0",
    "moment-mini": "^2.22.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.2",
    "@angular-devkit/build-ng-packagr": "^0.10.2",
    "@angular/cli": "^7.0.2",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@types/jest": "^22.2.3",
    "core-js": "^2.5.4",
    "jest": "^22.4.3",
    "moment-mini": "^2.22.1",
    "ng-packagr": "^4.4.0",
    "rxjs": "^6.0.0-rc.0",
    "ts-jest": "^23.10.4",
    "tsickle": "^0.33.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "^3.1.3",
    "zone.js": "^0.8.24"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "\\.spec\\.ts$"
  }
}
